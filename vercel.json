{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*\\.(js|css|html|ico|png|jpg|svg))",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}